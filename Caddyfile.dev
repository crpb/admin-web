# This assumes this project runs webpack-dev-server on 127.0.0.1:3065.

https://:8411 {
	errors stderr
	log stdout

	tls self_signed

	gzip

	proxy /api/v1/ https://d3u9fxr8qqc6qs.cloudfront.net

	proxy /sockjs-node/ 127.0.0.1:3065 {
		websocket
	}
	proxy /static 127.0.0.1:3065

	proxy / 127.0.0.1:3065
}
